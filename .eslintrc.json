{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "overrides": [
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    },
    "settings": {
        "import/resolver": {
          "alias": {
            "map": [
              [
                "@Components",
                "./src/components/index.js"
              ],
              [
                "@API",
                "./src/network/apis/APIs.js"
              ],
              [
                "@Actions",
                "./src/store/actions/index.js"
              ],
              [
                "@Store",
                "./src/store/index.js"
              ],
              [
                "@Config",
                "./src/config/index.js"
              ],
              [
                "@Pages",
                "./src/pages/index.js"
              ]
            ],
            "extensions": [
              ".ts",
              ".js",
              ".jsx",
              ".json"
            ]
          }
        }
      }
}
